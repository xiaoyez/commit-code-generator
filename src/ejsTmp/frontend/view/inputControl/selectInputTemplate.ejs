<el-select v-model="<%=
info.modelPath %>" placeholder="<%= info.placeholder
%>" clearable filterable<%
if (info.displayJudge) {
%> :display="<%= info.displayJudge %>"<%
} %>>
  <el-option v-for="<%=
    info.iterName %> in <%= info.listName
    %>" :key="<%= info.iterName %>.<%= info.keyFiled
    %>" :label="<%= info.iterName %>.<%= info.label
    %>" :value="<%= info.iterName %>.<%= info.value
  %>"/>
</el-select>