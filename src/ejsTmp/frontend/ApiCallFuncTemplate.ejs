export function <%= api.apiName %>(<% if (api.params) {
%>params: <%= tsTypeString(api.params) %><%
} %>) {
    return request<<%=
    api.result ? tsTypeString(api.result) : 'void'
    %>>({
        url: "<%= baseUrl + api.url %>",
        method: "<%= api.method %>",<% if (api.params) {%>
        <%=
        api.method === 'get' ? 'params' : 'data'
        %>: params<% } %>
    });
}